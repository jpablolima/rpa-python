   "execution_count": 13,
   "metadata": {},
   "outputs": [],
   "source": [
    "pyautogui.PAUSE = 1\n",
    "pyautogui.press(\"winleft\")\n",
    "pyautogui.write(\"steam\")\n",
    "pyautogui.press(\"enter\")\n",
    "time.sleep(10)\n",
    "pyautogui.alert(\"Steam aberto com sucesso!\")\n",
    "\n"
